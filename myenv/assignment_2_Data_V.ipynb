    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "x = np.arange(0,5,0.25)\n",
    "y1 = x *2\n",
    "y2 = x**2 * 2\n",
    "y3 = 2**x * 5\n",
    "\n",
    "plt.plot(x, y1, '\''g--'\'', x,y2, '\''bs'\'',x, y3, '\''r^'\'') # green dashed, blue squares and red triangle\n",
    "plt.xlabel(\"X-axis\")\n",
    "plt.ylabel(\"Y-axis\")\n",
    "plt.title(\"Multiple Lines with Different Styles\")\n",
    "plt.grid(True)\n",
    "plt.show()"
